#include<stdio.h>
#include<stdlib.h>
#include <ctype.h>
#include <math.h>

void in_values(int size,int array[size][size]);
void print_matrix(int size,int array[size][size]);

int main()
{
    int order=0;
    printf("Enter matrix size: ");
    scanf("%d", &order);
    int matrix1[order][order],matrix2[order][order];
    printf("\n");
    
    //>>>>>>>>>>>>>ENTER MATRIX VALUES>>>>>>>>>>>>>
    printf("MATRIX 1: \n");
    in_values(order,matrix1);
    //>>>>>>>>>>>>>PRINT MATRIX>>>>>>>>>>>>>
    printf("MATRIX 1: \n");
    print_matrix(order,matrix1);
    return 0;
}
//>>>>>>>>>>>>>ENTER MATRIX FUNCTION>>>>>>>>>>>>>
void in_values(int size,int array[size][size])
{
    for(int i=0;i<size;i++)
    {
        for(int j=0;j<size;j++)
        {
           printf("[%d][%d]= ",i,j);
           scanf("%d",&array[i][j]);
        }
    }
    printf("\n");
}
//>>>>>>>>>>>>>PRINT MATRIX FUNCTION>>>>>>>>>>>>>
void print_matrix(int size,int array[size][size])
{
    for(int i=0;i<size;i++)
    {
        for(int j=0;j<size;j++)
        {
           printf("[%d] ",array[i][j]);
        }
    printf("\n");
    }
}

